{
  "name": "ng-docs",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href=./ --aot=true --build-optimizer=true --optimization=true --common-chunk=false --stats-json=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ng-docs:serve-ssr",
    "serve:ssr": "node dist/ng-docs/server/main.js",
    "build:ssr": "ng build --prod && ng run ng-docs:server:production",
    "prerender": "ng run ng-docs:prerender",
    "bundle-report": "webpack-bundle-analyzer dist/ng-docs/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/cdk": "^11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/platform-server": "~11.2.8",
    "@angular/router": "~11.2.8",
    "@nguniversal/express-engine": "^11.2.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/katex": "^0.11.1",
    "@types/marked": "^3.0.2",
    "@types/mermaid": "^8.2.7",
    "@types/prettier": "^2.4.1",
    "express": "^4.15.2",
    "highlight.js": "^11.3.1",
    "katex": "^0.13.18",
    "marked": "^3.0.8",
    "mermaid": "^8.13.3",
    "ng-marked-editor": "^0.0.11",
    "ng-marked-preview": "^0.0.11",
    "prettier": "^2.4.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular/cli": "~11.2.7",
    "@angular/compiler-cli": "~11.2.8",
    "@angular/localize": "^11.2.14",
    "@nguniversal/builders": "^11.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
